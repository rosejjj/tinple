*{margin:0;padding:0}html,body{background:#efefef;width:100%}html section,body section{margin:10px}html section>div:nth-of-type(2) h4,body section>div:nth-of-type(2) h4{padding-left:18.5px}html section>div,body section>div{width:100%;margin-bottom:10px;background-color:#ffffff;box-shadow:0 2px 10px 3px rgba(170,170,170,0.3);border-radius:10px;overflow:hidden;font-size:12px}html section>div>div,body section>div>div{background-size:cover;background-position:center;width:100%;height:120px;background-color:rgba(193,210,240,0.8)}html section>div>h4,body section>div>h4{margin-top:10px;margin-left:10px;margin-bottom:5px;display:inline-block;position:relative}html section>div>h4 img,body section>div>h4 img{width:25px;height:25px;display:inline-block;position:absolute;top:-6px;left:-6px}html section>div>p,body section>div>p{margin-bottom:10px;margin-left:10px;color:#AAAAAA;font-weight:bold;width:95%;word-break:break-all}